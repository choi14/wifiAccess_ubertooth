 490:../common/ubertooth.c **** /* take control of R8C microcontroller and CC2400 on the ToorCon 13 badge */
 491:../common/ubertooth.c **** #ifdef TC13BADGE
 492:../common/ubertooth.c **** void r8c_takeover(void)
 493:../common/ubertooth.c **** {
 494:../common/ubertooth.c **** 	/* wait until user presses SW1 while USB is connected */
ARM GAS  /tmp/ccQ7rs84.s 			page 37


 495:../common/ubertooth.c **** 	while (SW1 || !VBUS) {
 496:../common/ubertooth.c **** 		/* turn off power to all peripherals except GPIO */
 497:../common/ubertooth.c **** 		PCONP = PCONP_PCGPIO;
 498:../common/ubertooth.c **** 
 499:../common/ubertooth.c **** 		/* enable interrupt on falling edge of P2.8 (SW1) */
 500:../common/ubertooth.c **** 		IO2IntEnF |= (0x1 << 8);
 501:../common/ubertooth.c **** 		ISER0 |= ISER0_ISE_EINT3;
 502:../common/ubertooth.c **** 
 503:../common/ubertooth.c **** 		/* go into power-down mode and wait for interrupt */
 504:../common/ubertooth.c **** 		SCB_SCR = SCB_SCR_SLEEPDEEP;
 505:../common/ubertooth.c **** 		PCON = (PCON_PM0 | PCON_BOGD);
 506:../common/ubertooth.c **** 		asm("WFI");
 507:../common/ubertooth.c **** 
 508:../common/ubertooth.c **** 		/* disable interrupt */
 509:../common/ubertooth.c **** 		IO2IntEnF &= ~(0x1 << 8);
 510:../common/ubertooth.c **** 		ICER0 |= ICER0_ICE_EINT3;
 511:../common/ubertooth.c **** 
 512:../common/ubertooth.c **** 		/* turn power back on for peripherals we use */
 513:../common/ubertooth.c **** 		PCONP = (PCONP_PCTIM0 | PCONP_PCSSP1 | PCONP_PCGPIO | PCONP_PCSSP0);
 514:../common/ubertooth.c **** 	}
 515:../common/ubertooth.c **** 
 516:../common/ubertooth.c **** 	/* drop R8C_CTL to let the R8C know we are taking over */
 517:../common/ubertooth.c **** 	R8C_CTL_CLR;
 518:../common/ubertooth.c **** 
 519:../common/ubertooth.c **** 	/* wait for the R8C to acknowledge the takeover */
 520:../common/ubertooth.c **** 	while (R8C_ACK);
 521:../common/ubertooth.c **** }
